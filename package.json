{
  "name": "movabletype",
  "version": "1.0.0",
  "description": "Welcome to Movable Type",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "vitest",
    "build-mt-css-admin2023": "npm run build-mt-css-compile-admin2023 && npm run build-mt-css-prefix-admin2023 && npm run build-mt-css-minify-admin2023",
    "build-mt-css-compile-admin2023": "sass --style expanded --source-map --embed-sources --precision 6 scss/admin2023/mt.scss mt-static/css/admin2023/mt.css",
    "build-mt-css-minify-admin2023": "cleancss -O1 --source-map --with-rebase --source-map-inline-sources --output mt-static/css/admin2023/mt.min.css mt-static/css/admin2023/mt.css",
    "build-mt-css-prefix-admin2023": "postcss --config build/postcss.config.js --replace mt-static/css/admin2023/mt.css",
    "build-mt-css-admin2025": "npm run build-mt-css-compile-admin2025 && npm run build-mt-css-prefix-admin2025 && npm run build-mt-css-minify-admin2025",
    "build-mt-css-compile-admin2025": "sass --style expanded --source-map --embed-sources --precision 6 scss/admin2025/mt.scss mt-static/css/admin2025/mt.css",
    "build-mt-css-minify-admin2025": "cleancss -O1 --source-map --with-rebase --source-map-inline-sources --output mt-static/css/admin2025/mt.min.css mt-static/css/admin2025/mt.css",
    "build-mt-css-prefix-admin2025": "postcss --config build/postcss.config.js --replace mt-static/css/admin2025/mt.css",
    "build-riot-admin2023": "npm run build-riot-listing-admin2023; npm run build-riot-contenttype-admin2023",
    "build-riot-listing-admin2023": "riot mt-static/js/admin2023/listing/tag mt-static/js/admin2023/listing/listing.js",
    "build-riot-contenttype-admin2023": "riot mt-static/js/admin2023/contenttype/tag mt-static/js/admin2023/contenttype/contenttype.js",
    "build-riot-admin2025": "npm run build-riot-listing-admin2025; npm run build-riot-contenttype-admin2025",
    "build-riot-listing-admin2025": "riot mt-static/js/admin2025/listing/tag mt-static/js/admin2025/listing/listing.js",
    "build-riot-contenttype-admin2025": "riot mt-static/js/admin2025/contenttype/tag mt-static/js/admin2025/contenttype/contenttype.js",
    "build-ss": "riot mt-static/js/ss/tag mt-static/js/ss/ss.js",
    "copy": "npm run copy-a-table && npm run copy-bootstrap5 && npm run copy-jquery-ui && npm run copy-jquery-ui-touch-punch && npm run copy-popper && npm run copy-riot && npm run rename-riot-compiler && npm run copy-svg4everybody && npm run copy-mt-chart-api && npm run copy-jquery && npm run copy-tinymce6 && npm run copy-tinymce6-language",
    "copy-a-table": "cp -rf node_modules/a-table/build mt-static/a-table.js/ && cp -rf node_modules/a-table/css mt-static/a-table.js/ && cp -rf node_modules/a-table/fonts mt-static/a-table.js",
    "copy-bootstrap5": "cp -rf node_modules/bootstrap5/dist/js/bootstrap.bundle.* mt-static/bootstrap5/js/",
    "copy-jquery-ui": "cp -f node_modules/jquery-ui/dist/*.js mt-static/jqueryui/",
    "copy-jquery-ui-touch-punch": "cp -rf node_modules/jquery-ui-touch-punch/jquery.ui.touch-punch.* mt-static/jqueryui/",
    "copy-popper": "cp -rf node_modules/popper.js/dist/umd/* mt-static/popper/",
    "copy-riot": "cd node_modules/riot && cp -f LICENSE.txt riot.js riot.min.js riot+compiler.js riot+compiler.min.js ../../mt-static/riot/",
    "rename-riot-compiler": "cd mt-static/riot && mv riot+compiler.js riot-compiler.js && mv riot+compiler.min.js riot-compiler.min.js",
    "copy-svg4everybody": "cd node_modules/svg4everybody/dist && cp -f svg4everybody.js svg4everybody.min.js ../../../mt-static/svg4everybody/",
    "copy-mt-chart-api": "cp -rf node_modules/mt-chart-api/lib/* mt-static/chart-api/",
    "copy-jquery": "npm run copy-jquery-core && npm run copy-jquery-migrate && npm run copy-jquery-validation",
    "copy-jquery-core": "cp -rf node_modules/jquery/dist/jquery.js mt-static/jquery/ && cp -rf node_modules/jquery/dist/jquery.min.* mt-static/jquery/",
    "copy-jquery-migrate": "cp -rf node_modules/jquery-migrate/dist/jquery-migrate.* mt-static/jquery/",
    "copy-jquery-validation": "cp -rf node_modules/jquery-validation/dist/jquery.validate.* mt-static/jquery/",
    "copy-tinymce6": "cp -rf node_modules/tinymce6/*.js node_modules/tinymce6/themes node_modules/tinymce6/skins node_modules/tinymce6/plugins node_modules/tinymce6/icons node_modules/tinymce6/models node_modules/tinymce6/license.txt mt-static/plugins/TinyMCE6/tiny_mce/",
    "copy-tinymce6-language": "rm -rf mt-static/plugins/TinyMCE6/tiny_mce/langs; mkdir mt-static/plugins/TinyMCE6/tiny_mce/langs; cp -rf node_modules/tinymce6-i18n/langs6/* mt-static/plugins/TinyMCE6/tiny_mce/langs/",
    "copy-codemirror": "cp -rf node_modules/codemirror/addon node_modules/codemirror/keymap node_modules/codemirror/lib node_modules/codemirror/mode node_modules/codemirror/theme mt-static/codemirror/",
    "copy-js-cookie": "cp -rf node_modules/js-cookie/dist/js.cookie.min.js mt-static/js-cookie/",
    "test-riot": "docker run --rm -v $PWD:/mt -w /mt node:8 bash -c \"npm install -g riot-cli && npm run build-riot && npm run build-ss\" && test `git status -s | wc -l` = 0",
    "build-svelte:app": "rollup --config rollup.config.ts --configPlugin typescript",
    "build-svelte": "npm run build-svelte:app",
    "svelte-dev": "rollup --config rollup.config.ts --configPlugin typescript -w",
    "svelte-check-types": "tsc",
    "svelte-format": "prettier --plugin prettier-plugin-svelte --write 'src/**/*.{ts,svelte}' '*.ts'",
    "svelte-lint": "eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/movabletype/movabletype.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/movabletype/movabletype/issues"
  },
  "homepage": "https://github.com/movabletype/movabletype#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/jquery": "^3.5.30",
    "@types/jqueryui": "^1.12.22",
    "@types/node": "^20.12.11",
    "@types/bootstrap": "^5.2.10",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.46.0",
    "jsdom": "^25.0.1",
    "postcss-cli": "^11.0.0",
    "postcss-flexbugs-fixes": "^5.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "rollup": "^4.28.0",
    "rollup-plugin-css-only": "^4.5.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.2",
    "sass": "1.77.6",
    "svelte": "^4.2.19",
    "svelte-check": "^4.3.3",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "@riotjs/observable": "^4.1.1",
    "a-table": "github:movabletype/a-table.js#v1.5.10-3",
    "bootstrap5": "npm:bootstrap@5.3.5",
    "codemirror": "5.61.1",
    "jquery": "3.7.0",
    "jquery-migrate": "3.5.2",
    "jquery-ui": "1.14.1",
    "jquery-ui-touch-punch": "github:movabletype/jquery-ui-touch-punch",
    "jquery-validation": "1.20.0",
    "js-cookie": "3.0.1",
    "lru-cache": "^10.4.3",
    "mt-chart-api": "github:movabletype/mt-chart-api-sdk-js#v1.0.2",
    "popper.js": "^1.16.1",
    "riot": "3.7.4",
    "svelte-portal": "^2.2.1",
    "svg4everybody": "^2.1.9",
    "tinymce6": "npm:tinymce@^6.8.5",
    "tinymce6-i18n": "npm:tinymce-i18n@23.6.19",
    "tslib": "^2.6.3"
  }
}
