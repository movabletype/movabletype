<table>
    <thead>
        <tr>
            <th><__trans phrase="Key"></th>
            <th><__trans phrase="Value"></th>
        </tr>
    </thead>
    <tbody>
<mt:Loop name="mt_cache_profile" sort_by="key">
    <mt:If name="__odd__">
        <tr class="djDebugOdd">
            <td><mt:Var name="__value__" escape="html"></td>
    <mt:Else>
            <td><mt:Var name="__value__" escape="html"></td>
        </tr>
    </mt:If>
</mt:Loop>
    </tbody>
</table>
