<table>
    <thead>
        <tr>
            <th><__trans phrase="Key"></th>
            <th><__trans phrase="Value"></th>
        </tr>
    </thead>
    <tbody>
<mt:Loop name="mt_headers" sort_by="key">
    <mt:If name="__odd__">
        <tr class="djDebugOdd">
    <mt:Else>
        <tr class="djDebugEven">
    </mt:If>
            <td><mt:Var name="__key__"></td>
            <td><mt:Var name="__value__" escape="html"></td>
        </tr>
</mt:Loop>
    </tbody>
</table>
