<mt:setvarblock name="page_title"><__trans phrase="An error occurred"></mt:setvarblock>
<mt:setvar name="complete" value="1">
<mt:include name="include/chromeless_header.tmpl">
<mtapp:statusmsg
   id="generic-error"
   class="error"
   can_close="0">
  <mt:var name="error">
</mtapp:statusmsg>
<mt:unless var="hide_goback_button">
<div class="actions-bar actions-bar-login">
  <button
     onclick="<mt:var name="goback">"
     accesskey="s"
     title="<__trans phrase="Back (s)">"
     class="go-back action primary button">
    <__trans phrase="Back">
  </button>
</div>
</mt:unless>
<mt:include name="include/chromeless_footer.tmpl">
